  {
    "struct_name": "PumpParams",
    "docstring": "Pump characteristics, containing both inp-file data and normalized representation for head, efficiency, and power curves. Currently, only single-speed",
    "supertype": "TechnicalParams",
    "fields": [
      {
        "name": "epnt_type",
        "comment": "epanet categorizes pump specs as HEAD or POWER",
        "data_type": "Union{Nothing, String}",
        "null_value": "nothing"
      },
      {
        "name": "epnt_power",
        "comment": "specified constant power if type POWER",
        "data_type": "Union{Nothing, Float64}",
        "null_value": "nothing"
      },
      {
        "name": "epnt_head",
        "comment": "provided head curve if type HEAD",
        "data_type": "Union{Nothing, Array{Tuple{Float64,Float64},1}}",
        "null_value": "nothing"
      },
      {
        "name": "epnt_efficiency",
        "comment": "provided efficiency curve (or single value) if type HEAD",
        "data_type": "Union{Nothing, Float64, Array{Tuple{Float64,Float64},1}}",
        "null_value": "nothing"
      },
      {
        "name": "flowBEP",
        "comment": "flow rate at the best efficiency point",
        "data_type": "Float64",
        "null_value": "0.0"
      },
      {
        "name": "effncyBEP",
        "comment": "efficiency at the best efficiency point",
        "data_type": "Float64",
        "null_value": "0.0"
      },
      {
        "name": "headBEP",
        "comment": "head at the best efficiency point",
        "data_type": "Float64",
        "null_value": "0.0"
      },
      {
        "name": "head0",
        "comment": "head ratio G0/GBEP",
        "data_type": "Float64",
        "null_value": "1.25"
      },
      {
        "name": "powerslope",
        "comment": "slope of the linear power curve",
        "data_type": "Float64",
        "null_value": "0.0"
      },
      {
        "name": "powerintcpt",
        "comment": "inetercept of the linear power curve",
        "data_type": "Float64",
        "null_value": "0.0"
      },
      {
        "name": "internal",
        "data_type": "InfrastructureSystemsInternal"
      }
    ]
  },   
